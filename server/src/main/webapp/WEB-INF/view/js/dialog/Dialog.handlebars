{{#if header}}
    <div class="modal-header">
        <button type="button" class="close btn-close-dialog" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3>{{header}}</h3>
    </div>
{{/if}}
<div class="modal-body">
</div>
<div class="modal-footer">
    {{#if cancelLael}}
        <a href="#" class="btn btn-close-dialog">{{cancelLael}}</a>
    {{/if}}
    {{#if confirmLabel}}
        <a href="#" class="btn btn-primary btn-confirm-dialog">{{confirmLabel}}</a>
    {{/if}}
</div>
